{
	"name": "markdown-it-mdeqn-be",
	"version": "0.5.1",
	"description": "Basic Geometric library for Type Design",
	"engineStrict": true,
	"engines": {
		"node": ">= 8.9.0"
	},
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"devDependencies": {
		"@types/node": "~8.5.10",
		"rimraf": "~2.6.2",
		"tslint": "^5.10.0",
		"typescript": "^3.0.0",
		"prettier": "^1.18.2"
	},
	"scripts": {
		"clean": "rimraf coverage lib tmp",
		"build": "tsc -p tsconfig.json",
		"watch": "tsc -w -p tsconfig.json",
		"test": "ava --verbose",
		"prepack": "npm run clean && tsc -p tsconfig.release.json",
		"doc": "typedoc --out ./doc --theme markdown src"
	},
	"author": "Renzhi Li aka Belleve Invis<belleve@typeof.net>",
	"license": "MIT",
	"dependencies": {
		"param-case": "^2.1.1",
		"tslib": "~1.9.0"
	},
	"ava": {
		"files": [
			"lib/**/tests/*.js",
			"lib/**/test.js"
		]
	}
}
